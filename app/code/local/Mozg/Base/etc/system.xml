                                <export_settings translate="label" module="mozg_base">
                                    <label></label>
                                    <comment></comment>
                                    <button_label>Exportar configurações</button_label>
                                    <frontend_model>mozg_base/adminhtml_system_config_exportSettings</frontend_model>
                                    <sort_order>10</sort_order>
                                    <show_in_default>1</show_in_default>
                                    <show_in_website>1</show_in_website>
                                    <show_in_store>0</show_in_store>
                                </export_settings>
                                <debug_block_name translate="label">
                                    <label>Exibir nomes dos blocos</label>
                                    <comment><![CDATA[<a href="https://github.com/mozgbrasil/magento-base-php_72#-exibir-nomes-dos-blocos" target="_blank">__?__</a>]]></comment>
                                    <frontend_type>select</frontend_type>
                                    <source_model>adminhtml/system_config_source_yesno</source_model>
                                    <config_path>mozg_general/resources/debug_block_name</config_path>
                                    <sort_order>20</sort_order>
                                    <show_in_default>1</show_in_default>
                                    <show_in_website>1</show_in_website>
                                    <show_in_store>1</show_in_store>
                                </debug_block_name>
                            </fields>
                        </mozg_base_group_2>
                    </fields>
                </resources>
            </groups>
        </mozg_general>
