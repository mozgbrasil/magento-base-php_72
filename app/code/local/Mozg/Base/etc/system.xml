<config>
    <tabs>
        <mozg translate="label" module="mozg_base">
            <label>Mozg</label>
            <sort_order>8000</sort_order>
        </mozg>
    </tabs>
    <sections>
        <mozg_general translate="label" module="mozg_base">
            <header_css>header-section-mozg_general</header_css>
            <class>separator-top section-mozg_general</class>
            <label>General</label>
            <tab>mozg</tab>
            <frontend_type>text</frontend_type>
            <sort_order>100</sort_order>
            <show_in_default>1</show_in_default>
            <show_in_website>1</show_in_website>
            <show_in_store>1</show_in_store>
            <groups>
                <resources translate="label" module="mozg_base">
                    <label>Configurações Padrão</label>
                    <frontend_type>text</frontend_type>
                    <sort_order>10</sort_order>
                    <show_in_default>1</show_in_default>
                    <show_in_website>1</show_in_website>
                    <show_in_store>1</show_in_store>
                    <expanded>1</expanded>
                    <fields>
                        <mozg_base_group_1 type="group" translate="label comment tooltip" module="mozg_base">
                            <label>Recursos</label>
                            <frontend_type>text</frontend_type>
                            <show_in_default>1</show_in_default>
                            <show_in_website>1</show_in_website>
                            <show_in_store>1</show_in_store>
                            <sort_order>10</sort_order>
                            <frontend_model>mozg_base/adminhtml_system_config_fieldset_expanded</frontend_model>
                            <fields>
                                <active_split_price translate="label" module="mozg_base">
                                    <label>Exibir parcela do produto</label>
                                    <comment><![CDATA[<a href="https://github.com/mozgbrasil/magento-base-php_72#-ativar-estilo-ao-iwd_opc" target="_blank">__?__</a>]]></comment>
                                    <frontend_type>select</frontend_type>
                                    <source_model>adminhtml/system_config_source_yesno</source_model>
                                    <config_path>mozg_general/resources/active_split_price</config_path>
                                    <sort_order>19</sort_order>
                                    <show_in_default>1</show_in_default>
                                    <show_in_website>1</show_in_website>
                                    <show_in_store>1</show_in_store>
                                </active_split_price>
                                <active_style_iwd_opc translate="label" module="mozg_base">
                                    <label>Ativar estilo ao IWD_Opc</label>
                                    <comment><![CDATA[<a href="https://github.com/mozgbrasil/magento-base-php_72#-ativar-estilo-ao-iwd_opc" target="_blank">__?__</a>]]></comment>
