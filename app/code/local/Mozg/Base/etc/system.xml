                                    <show_in_default>1</show_in_default>
                                    <show_in_website>1</show_in_website>
                                    <show_in_store>1</show_in_store>
                                </active_button_google_route_order>
                                <active_new_columns_sales_order translate="label" module="mozg_base">
                                    <label>Exibir novas colunas na grade de pedidos</label>
