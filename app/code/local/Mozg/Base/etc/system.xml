                                    <comment><![CDATA[<a href="https://github.com/mozgbrasil/magento-base-php_72#-" target="_blank">__?__</a>]]></comment>
                                    <frontend_type>select</frontend_type>
                                    <source_model>adminhtml/system_config_source_yesno</source_model>
                                    <config_path>mozg_general/resources/active_boxpacker_sales_order_view</config_path>
