                                    <show_in_default>1</show_in_default>
                                    <show_in_website>1</show_in_website>
                                    <show_in_store>1</show_in_store>
                                </active_1column_checkout_onepage_success>
                                <active_boxpacker_sales_order_view translate="label" module="mozg_base">
